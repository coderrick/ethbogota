{

  "description": "Comet, is a browser extension that uses the news in addition to pricing data to help you make those one in a lifetime trades.",
  "manifest_version": 2,
  "name": "Comet",
  "version": "1.0",
  "homepage_url": "https://github.com/coderrick/ethbogota.git",
  "icons": {
    "48": "icons/comet.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/comet.png",
    "theme_icons": [{
        "light": "icons/comet.png",
        "dark": "icons/comet.png",
        "size": 32
    }],
    "default_title": "Add-O(n)",
    "default_popup": "popup/add_on_panel.html"
  },

  "web_accessible_resources": [
    "charts/*.jpg"
  ]

}
